    "@tsed/engines": "^7.18.2",
    "@tsed/exceptions": "^7.18.2",
    "@tsed/json-mapper": "^7.18.2",
    "@tsed/logger": "^6.3.4",
    "@tsed/logger-file": "^6.3.4",
    "@tsed/openspec": "^7.18.2",
    "@tsed/passport": "^7.18.2",
    "@tsed/platform-cache": "^7.18.2",
    "@tsed/platform-exceptions": "^7.18.2",
    "@tsed/platform-express": "^7.18.2",
    "@tsed/platform-log-middleware": "^7.18.2",
    "@tsed/platform-middlewares": "^7.18.2",
    "@tsed/platform-params": "^7.18.2",
    "@tsed/platform-response-filter": "^7.18.2",
    "@tsed/platform-views": "^7.18.2",
    "@tsed/schema": "^7.18.2",
    "@tsed/typegraphql": "^7.18.2",
    "ajv": "^8.12.0",
    "apollo-datasource": "^3.3.1",
    "apollo-datasource-rest": "^3.5.1",
    "apollo-server-core": "^3.6.2",
    "apollo-server-express": "2.25.2",
    "barrelsby": "^2.5.1",
    "body-parser": "^1.20.2",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "graphql": "^15.7.2",
