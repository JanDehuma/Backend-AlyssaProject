{
  "name": "alyssa-project",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "yarn run barrels && npx tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "yarn run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "dev": "yarn generate && nodemon 'src/index.ts' --exec \"npx ts-node src/index.ts\" -e ts,graphql"
  },
  "dependencies": {
    "@tsed/ajv": "^7.18.2",
    "@tsed/common": "^7.18.2",
    "@tsed/core": "^7.18.2",
