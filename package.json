    "@tsed/cli-plugin-eslint": "4.4.2",
    "@tsed/cli-plugin-passport": "4.4.2",
    "@tsed/cli-plugin-typegraphql": "4.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
